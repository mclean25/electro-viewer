{
  "name": "electro-viewer",
  "version": "0.1.16",
  "description": "A browser-based viewer for ElectroDB entities and DynamoDB data",
  "main": "index.js",
  "type": "module",
  "bin": {
    "electro-viewer": "./bin/electro-viewer.js"
  },
  "files": [
    "bin",
    "dist",
    "server-runtime.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm build:cache-cli",
    "build:cache-cli": "esbuild src/build-cache-cli.ts --bundle --platform=node --format=esm --outfile=dist/build-cache.js --packages=external",
    "prepublishOnly": "pnpm build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check:write": "biome check --write --unsafe ."
  },
  "keywords": [
    "electrodb",
    "dynamodb",
    "aws",
    "database",
    "viewer",
    "browser",
    "query",
    "entity",
    "cli"
  ],
  "author": "Alex McLean",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mclean25/electro-viewer.git"
  },
  "homepage": "https://github.com/mclean25/electro-viewer",
  "bugs": {
    "url": "https://github.com/mclean25/electro-viewer/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.974.0",
    "@aws-sdk/credential-providers": "^3.974.0",
    "@aws-sdk/lib-dynamodb": "^3.974.0",
    "@biomejs/biome": "^2.3.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/react-start": "^1.159.5",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/router-core": "^1.159.4",
    "@vitejs/plugin-react": "^5.1.2",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "electrodb": "^3.5.3",
    "esbuild": "^0.25.12",
    "fast-glob": "^3.3.3",
    "lucide-react": "^0.544.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "sst": "^3.17.37",
    "vitest": "^3.2.4"
  }
}
